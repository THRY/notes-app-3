<div class="note-editor">
  <div class="container">
      <h2>Edit note</h2>
      <form data-id="{{note._id}}">
        <label for="title">Title</label>
        <input type=text id="title" value="{{note.title}}"></input>
        <label for="title">Description</label>
        <textarea rows="10" id="description" >{{note.description}}</textarea>
      </form>
      <span class="button edit save"><a class="back" href="/"></a><label>save</label></a></span>
  </div>
</div>